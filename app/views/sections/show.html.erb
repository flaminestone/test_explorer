<div class="container">
  <div id="classHeader">
    <h3>Tests</h3>
  </div>
  <% if user_signed_in? %>
      <div class="row">
        <div class="btn-group btn-group-justified" role="group">
          <div class="btn-group" role="group">
            <%= button_to 'Create new section', new_section_path, :class => 'btn btn-success' %>
          </div>
        </div>
      </div>
  <% end %>
  <div class="row">

      <% @tests.each do |test| %>
          <div class="section_item">
            <div class="col-xs-9"><a id="block_element" href="<%= section_test_path(@section, test) %>"> <%= test.name %></a></div>
          </div>
          <div class="col-xs-2"><a id="block_element" href="">&#9200;<%= test.time_out %> min </a></div>
          <div class="col-xs-1"><a id="block_element" href="">
            <% if test.come_back %>
                &#9668;
            <% else %>
                &#8855;
            <% end %>
          </a>
          </div>

      <% end %>
      </div>
</div>
